= form_for [current_owner, @store] do |f|
  %div
    = f.label :store_name
    = f.text_field :name
  %div
    = f.label :city
    = f.text_field :city
  %div
    = f.label :state
    = f.text_field :state
  %div
    = f.label :address
    = f.text_field :address
  %div
    = f.label :country
    = f.text_field :country
  %div
    = f.label :zip_code
    = f.text_field :zip_code
  %div
    = f.label :contact_email
    = f.text_field :contact_email
  %div
    = f.label :contact_phone_number
    = f.text_field :contact_phone_number
  %div
    = f.label :type
    = f.text_field :type
  %div
    = f.submit
